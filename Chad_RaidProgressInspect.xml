<Ui xmlns="http://www.blizzard.com/wow/ui/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.blizzard.com/wow/ui/ ..\..\FrameXML\UI.xsd">
	<Script file="Chad_RaidProgressInspect.lua"/>
	<Frame name="RaidProgressButtonTemplate" mixin="RaidProgressInspectDisplayMixin" virtual="true">
		<Size x="300" y="70"/>
		<Layers>
			<Layer level="BORDER" >
				<FontString parentKey="raidDifficultyLabel" inherits="GameFontHighlight">
					<Anchors>
						<Anchor point="TOPLEFT" x="7" y="-8"/>
					</Anchors>
				</FontString>
				<FontString parentKey="raidMaxProg" inherits="SystemFont_Huge1_Outline" text="/10">
					<Anchors>
						<Anchor point="BOTTOMRIGHT" relativePoint="BOTTOMRIGHT" x="-10" y="18"/>
					</Anchors>
				</FontString>
				<FontString parentKey="raidCurrentProg" inherits="SystemFont_Huge1_Outline">
					<Anchors>
						<Anchor point="BOTTOMRIGHT" relativePoint="BOTTOMRIGHT" x="-46" y="18"/>
					</Anchors>
				</FontString>
			</Layer>
			<Layer level="BACKGROUND">
				<Texture parentKey="backTexture" atlas="pvpqueue-button-up"/>
				<Anchors>
					<Anchor point="TOP" relativeKey="$parent.II_RaidProgressFrame" y="-100"/>
				</Anchors>
			</Layer>
		</Layers>
		<Frames>
			<StatusBar parentKey="bossProgressBar" drawLayer="ARTWORK">
				<Size x="150" y="30"/>
				<Anchors>
					<Anchor point="CENTER" relativePoint="CENTER" x="-10" y="-5"/>
				</Anchors>
				<BarTexture file="Interface\TargetingFrame\UI-StatusBar"/>
				<BarColor r="0" g="1" b="0.10"/>
				<Layers>
					<Layer level="BACKGROUND">
						<Texture parentKey="Background">
							<BarTexture file="Interface\TargetingFrame\UI-StatusBar"/>
							<Color r="0.6" g="0.20" b="0.20" a="0.65"/>
						</Texture>
					</Layer>
				</Layers>
			</StatusBar>
		</Frames>
	</Frame>

	<Frame name="II_RaidProgressFrame" enableMouse="true" parent="InspectFrame" mixin="RaidProgressInspectLayoutMixin" hidden="true" useParentLevel="true" setAllPoints="true" id="6">
		<Size x="300" y="500" />
		<Layers>
			<Layer level="BACKGROUND">
				<Texture parentKey="BG" file="Interface\ENCOUNTERJOURNAL\UI-EJ-BACKGROUND-SanctumofDomination">
					<Size x="425" y="425"/>
					<Anchors>
						<Anchor point="TOPLEFT" x="8" y="-60"/>
					</Anchors>
				</Texture>
			</Layer>
		</Layers>
		<Frames>
			<Frame name="InspectRaidProgressBoxFrame">
				<Size x="300" y="325" />
				<Anchors>
					<Anchor point="TOP" relativeKey="$parent.II_RaidProgressFrame" relativePoint="TOP" y="-95"/>
				</Anchors>
			</Frame>		
			<Frame parentKey="RaidListDropDown" inherits="UIDropDownMenuTemplate" mixin="RaidDropdownMixin" virtual="true">
				<Size x="200" y="40"/>
				<Anchors>
					<Anchor point="TOP" relativeKey="$parent.II_RaidProgressFrame" relativePoint="TOP" x="100" y="-65"/>
				</Anchors>
				<Layers>
					<Layer level="OVERLAY">
						<FontString name="$parentRaidLabel" inherits="GameFontHighlight" text="Sanctum">
							<Anchors>
								<Anchor point="TOPLEFT" relativePoint="TOPLEFT">
								<Offset>
									<AbsDimension x="25" y="-6"/>
								</Offset>
								</Anchor>
							</Anchors>
						</FontString>
					</Layer>
				</Layers>
			</Frame>
		</Frames>
		
		<Scripts>
			<OnLoad method="OnLoad"/>
			<OnShow method="OnShow"/>
		</Scripts>
	</Frame>
</Ui>


